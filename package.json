{
  "name": "react-streamfield",
  "version": "0.8.1",
  "author": "NoriPyt <contact@noripyt.com> (https://noripyt.com)",
  "description": "Powerful field for inserting multiple blocks with nesting.",
  "keywords": [
    "react",
    "react-component",
    "field"
  ],
  "homepage": "https://github.com/noripyt/react-streamfield",
  "repository": {
    "type": "git",
    "url": "https://github.com/noripyt/react-streamfield"
  },
  "bugs": {
    "url": "https://github.com/noripyt/react-streamfield/issues"
  },
  "license": "BSD-3-Clause",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/react-streamfield.cjs.js",
  "module": "dist/react-streamfield.esm.js",
  "sideEffects": false,
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prettier-stylelint": "^0.4.2",
    "react-animate-height": "^2.0.7",
    "react-beautiful-dnd": "^10.0.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-viewport": "^4.1.7",
    "@storybook/react": "^4.1.7",
    "autoprefixer": "^9.4.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "css-loader": "^1.0.1",
    "custom-event-polyfill": "^1.0.6",
    "element-closest": "^3.0.1",
    "jest": "^23.6.0",
    "npx": "^10.2.0",
    "postcss-cli": "^6.1.1",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.1",
    "sass": "^1.16.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^8.4.0",
    "stylelint-scss": "^2.2.0",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "browserslist": [
    "Firefox ESR",
    "ie 11",
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Edge versions",
    "last 1 Firefox version",
    "last 2 iOS versions",
    "last 2 Safari versions"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/plugin-proposal-object-rest-spread",
      [
        "transform-react-remove-prop-types",
        {
          "mode": "unsafe-wrap",
          "ignoreFilenames": [
            "node_modules"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ]
      }
    }
  },
  "scripts": {
    "build": "rollup -c && build-storybook -c .storybook -s example -o public && sass src/scss/index.scss | npx postcss --use autoprefixer --no-map > dist/react-streamfield.css",
    "start": "start-storybook -c .storybook -s example -p 9001",
    "lib": "babel src --out-dir lib",
    "lib:watch": "babel -w src --out-dir lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build -s"
  }
}
